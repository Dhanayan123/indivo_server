{% load template_utils %}<?xml version="1.0" encoding="utf-8" ?>
<ConnectCredentials>
  <App id="{{app_email}}" />
  <ConnectToken>{{connect_token.token}}</ConnectToken>
  <ConnectSecret>{{connect_token.secret}}</ConnectSecret>
  <APIBase>{{api_base}}</APIBase>
  <RESTToken>{{rest_token.token}}</RESTToken>
  <RESTSecret>{{rest_token.secret}}</RESTSecret>
  <OAuthHeader>{{oauth_header}}</OAuthHeader>
  <ExpiresAt>{{rest_token.expires_at|format_iso8601_datetime}}</ExpiresAt>
</ConnectCredentials>
